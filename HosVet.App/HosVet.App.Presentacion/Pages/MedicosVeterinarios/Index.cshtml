@page
@model HosVet.App.Presentacion.Pages.MedicosVeterinarios.IndexModel
@{
}
<html>
    <head>
        <meta charset="utf-8">
        <title>Lista de Veterinarios</title>
    </head>
    <body>
        <h1>Administrar medicos veterinarios</h1><br>
        <ul class="submenu">
            <li><a href="/MedicosVeterinarios/Index">Lista de Veterinarios</a></li>
            <li><a href="/MedicosVeterinarios/NuevoVeterinario">Nuevo Veterinario</a></li>
        </ul>  
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Id persona</th>
                    <th scope="col">Codigo</th>
                    <th scope="col">Especialidad</th>
                    <th scope="col">Registro rethus</th>
                    <th scope="col">Cantidad de pacientes</th>
                </tr>
            </thead>
            @foreach (var mv in @Model.medicosVeterinarios)
            {
                <tr>
                    <td>@mv.Id</td>
                    <td>@mv.Codigo</td>
                    <td>@mv.Especialidad</td>
                    <td>@mv.RegistroRethus</td>
                    <td>@mv.CantidadPacientes</td>
                </tr>
            }
        </table>
    </body>
</html>